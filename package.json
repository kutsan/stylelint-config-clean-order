{
  "name": "stylelint-config-clean-order",
  "version": "8.0.1",
  "description": "Order your styles with stylelint-order.",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "stylelint-order",
    "property order",
    "css order"
  ],
  "repository": "github:kutsan/stylelint-config-clean-order",
  "license": "MIT",
  "author": "Kutsan Kaplan <me@kutsan.dev> (https://kutsan.dev)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./error": {
      "types": "./dist/error.d.ts",
      "import": "./dist/error.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm run clean",
    "build": "tsc --project tsconfig.build.json",
    "check": "pnpm run check:lint && pnpm run check:types && pnpm run check:format",
    "check:format": "prettier --check .",
    "check:lint": "eslint --max-warnings=0",
    "check:types": "tsc",
    "clean": "rimraf ./dist",
    "format": "prettier --write --list-different .",
    "prepare": "node scripts/install-husky.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@kutsan/eslint-config": "^1.4.0",
    "@kutsan/prettier-config": "^1.1.0",
    "@kutsan/typescript-config": "^1.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "csstype": "^3.2.3",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.2",
    "stylelint": "^16.26.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "peerDependencies": {
    "stylelint": ">=16",
    "stylelint-order": ">=6"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=24",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "onFail": "warn"
    }
  }
}
